{
  "buildCommand": "pnpm build:vercel",
  "outputDirectory": "dist/public",
  "framework": null,
  "devCommand": "pnpm dev",
  "installCommand": "pnpm install",
  "functions": {
    "api/**/*.ts": {
      "memory": 1024,
      "maxDuration": 60
    }
  },
  "routes": [
    {
      "src": "/api/trpc/(.*)",
      "dest": "/api/trpc"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
